{
  "name": "youtrack-markdown-snippet",
  "description": "A YouTrack Widget that allows you to embed content from different sources (issues, articles, custom content) directly into other YouTrack markdown",
  "author": "Damir Yusipov",
  "private": true,
  "version": "1.2.1",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "youtrack",
    "widget",
    "jetbrains",
    "markdown",
    "snippet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/udamir/youtrack-markdown-snippet.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.app.json && vite build && youtrack-app validate dist",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0",
    "test": "echo 'no tests'",
    "pack": "rm -f out/markdown-snippet.zip && mkdir -p out && cd dist && zip -r ../out/markdown-snippet.zip ./*",
    "upload:test": "dotenv -- bash -c 'youtrack-app upload dist --host $YOUTRACK_BASE_URL_TEST --token $YOUTRACK_TOKEN_TEST'",
    "upload": "dotenv -- bash -c 'youtrack-app upload dist --host $YOUTRACK_BASE_URL --token $YOUTRACK_TOKEN'"
  },
  "dependencies": {
    "@jetbrains/ring-ui-built": "^7.0.8",
    "markdown-it": "^14.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "youtrack-client": "^0.7.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@jetbrains/eslint-config": "^6.0.2",
    "@jetbrains/youtrack-apps-tools": "^0.0.1",
    "@types/highlightjs": "^9.12.6",
    "@types/markdown-it": "^14.1.2",
    "@types/mermaid": "^9.2.0",
    "@types/node": "^24.0.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.13.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.11.0",
    "terser": "^5.43.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^6.0.1",
    "vite-plugin-static-copy": "^2.2.0"
  }
}
